<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="list">
		<xs:complexType>
			<xs:sequence minOccurs="1" maxOccurs="1">
				<xs:element name="weapon" maxOccurs="9999" minOccurs="0">
					<xs:complexType>
						<xs:sequence minOccurs="1" maxOccurs="1">
							<xs:element name="price" maxOccurs="1" minOccurs="0">
								<xs:complexType>
									<xs:attribute name="id" use="required" type="xs:int" />
									<xs:attribute name="count" use="required" type="xs:int" />
								</xs:complexType>
							</xs:element>
							<xs:element name="removed" maxOccurs="1" minOccurs="0">
								<xs:complexType>
									<xs:attribute name="modifier" use="required" type="xs:decimal" />
								</xs:complexType>
							</xs:element>
							<xs:element name="skill" maxOccurs="unbounded" minOccurs="0">
								<xs:complexType>
									<xs:attribute name="id" use="required" type="xs:int" />
									<xs:attribute name="level" use="required" type="xs:int" />
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="id" use="required" type="xs:int" />
						<xs:attribute name="nameEn" use="required" type="xs:normalizedString" />
						<xs:attribute name="nameRu" type="xs:normalizedString" />
						<xs:attribute name="type" use="required" type="xs:normalizedString" />
						<xs:attribute name="allowEnchant" type="xs:boolean" />
						<xs:attribute name="allowAugment" type="xs:boolean" />
						<xs:attribute name="isForSell" type="xs:boolean" />
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>