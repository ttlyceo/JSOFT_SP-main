<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="list">
		<xs:complexType>
			<xs:sequence minOccurs="1" maxOccurs="1">
				<xs:element name="location" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence minOccurs="1" maxOccurs="unbounded">
							<xs:element name="class" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:attribute name="id" type="xs:string" />
								</xs:complexType>
							</xs:element>
							<xs:element name="level" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:attribute name="min" type="xs:integer" />
									<xs:attribute name="max" type="xs:integer" />
								</xs:complexType>
							</xs:element>
							<xs:element name="territory" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence minOccurs="1" maxOccurs="unbounded">
										<xs:element name="add" minOccurs="1" maxOccurs="unbounded">
											<xs:complexType>
												<xs:attribute name="x" type="xs:integer" />
												<xs:attribute name="y" type="xs:integer" />
												<xs:attribute name="zmin" type="xs:integer" />
												<xs:attribute name="zmax" type="xs:integer" />
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="name" type="xs:string" use="required" />
								</xs:complexType>
							</xs:element>
							<xs:element name="delay" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:attribute name="min" type="xs:integer" />
									<xs:attribute name="max" type="xs:integer" />
								</xs:complexType>
							</xs:element>
							<xs:element name="respawn" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:attribute name="min" type="xs:integer" />
									<xs:attribute name="max" type="xs:integer" />
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="id" type="xs:integer" use="required" />
						<xs:attribute name="amount" type="xs:integer" use="required" />
					</xs:complexType>
				</xs:element>

				<xs:element name="traders" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence minOccurs="1" maxOccurs="unbounded">
							<xs:element name="trader" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence minOccurs="0" maxOccurs="unbounded">
										<xs:element name="territory" minOccurs="0" maxOccurs="1">
											<xs:complexType>
												<xs:sequence minOccurs="1" maxOccurs="unbounded">
													<xs:element name="add" minOccurs="1" maxOccurs="unbounded">
														<xs:complexType>
															<xs:attribute name="x" type="xs:integer" />
															<xs:attribute name="y" type="xs:integer" />
															<xs:attribute name="zmin" type="xs:integer" />
															<xs:attribute name="zmax" type="xs:integer" />
														</xs:complexType>
													</xs:element>
												</xs:sequence>
												<xs:attribute name="name" type="xs:string" use="required" />
											</xs:complexType>
										</xs:element>
										<xs:element name="point" minOccurs="0" maxOccurs="1">
											<xs:complexType>
												<xs:attribute name="x" type="xs:integer" />
												<xs:attribute name="y" type="xs:integer" />
												<xs:attribute name="z" type="xs:integer" />
												<xs:attribute name="h" type="xs:integer" />
											</xs:complexType>
										</xs:element>
										<xs:element name="items" minOccurs="0" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence minOccurs="1" maxOccurs="unbounded">
													<xs:element name="add" minOccurs="0" maxOccurs="unbounded">
														<xs:complexType>
															<xs:attribute name="itemId" type="xs:integer" />
															<xs:attribute name="amount" type="xs:decimal" />
															<xs:attribute name="enchant" type="xs:integer" />
														</xs:complexType>
													</xs:element>
													<xs:element name="trade" minOccurs="0" maxOccurs="unbounded">
														<xs:complexType>
															<xs:attribute name="itemId" type="xs:integer" />
															<xs:attribute name="amountMin" type="xs:decimal" />
															<xs:attribute name="amountMax" type="xs:decimal" />
															<xs:attribute name="priceMin" type="xs:decimal" />
															<xs:attribute name="priceMax" type="xs:decimal" />
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="message" minOccurs="0" maxOccurs="unbounded">
											<xs:complexType>
												<xs:attribute name="val" type="xs:string" use="required" />
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="id" type="xs:integer" use="required" />
									<xs:attribute name="classId" type="xs:integer" use="required" />
									<xs:attribute name="type" type="xs:string" use="required" />
									<xs:attribute name="minLvl" type="xs:integer" />
									<xs:attribute name="maxLvl" type="xs:integer" />
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>